{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true
    },
    "extends": [
        "plugin:vue/recommended"
    ],
    "plugins": [
        "vue"
    ],
    "rules": {
        "indent":["error",4],
        "max-len":["error",{"code":120}],
        "no-console": "off"
    }
}